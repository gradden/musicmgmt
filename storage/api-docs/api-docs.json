{
    "openapi": "3.0.0",
    "info": {
        "title": "Musicmgmt API",
        "version": "1.0"
    },
    "servers": [
        {
            "url": "/api",
            "description": "API Server"
        }
    ],
    "paths": {
        "/users/me": {
            "get": {
                "tags": [
                    "User"
                ],
                "summary": "Get current user",
                "operationId": "5aa08d59e570c094302e54dcbd45cade",
                "responses": {
                    "200": {
                        "description": "OK",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/UserResource"
                                }
                            }
                        }
                    },
                    "401": {
                        "description": "Unauthorized"
                    }
                },
                "security": [
                    {
                        "bearerAuth": []
                    }
                ]
            }
        },
        "/auth/login": {
            "post": {
                "tags": [
                    "Auth"
                ],
                "summary": "Login",
                "operationId": "2e19250fcb6adcc6dc23cd2dd19489b6",
                "requestBody": {
                    "content": {
                        "application/json": {
                            "schema": {
                                "$ref": "#/components/schemas/LoginRequest"
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "OK",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/AuthResource"
                                }
                            }
                        }
                    },
                    "401": {
                        "description": "Unauthorized"
                    }
                }
            }
        },
        "/auth/register": {
            "post": {
                "tags": [
                    "Auth"
                ],
                "summary": "Registration",
                "operationId": "c0d615b1e4523cab480069cea2c590dd",
                "requestBody": {
                    "content": {
                        "application/json": {
                            "schema": {
                                "$ref": "#/components/schemas/RegisterRequest"
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "OK"
                    },
                    "422": {
                        "description": "Unprocessable entity"
                    }
                }
            }
        }
    },
    "components": {
        "schemas": {
            "LoginRequest": {
                "title": "LoginRequest",
                "description": "Login credentials",
                "required": [
                    "email",
                    "password"
                ],
                "properties": {
                    "email": {
                        "description": "Email",
                        "type": "string",
                        "example": "example@asd.com"
                    },
                    "password": {
                        "description": "Password",
                        "type": "string",
                        "example": "Teszt1234"
                    }
                },
                "type": "object"
            },
            "RegisterRequest": {
                "title": "RegisterRequest",
                "description": "Registration credentials",
                "required": [
                    "name",
                    "email",
                    "password"
                ],
                "properties": {
                    "name": {
                        "description": "Full name",
                        "type": "string",
                        "example": "Foo Bar"
                    },
                    "email": {
                        "description": "Email",
                        "type": "string",
                        "example": "example@asd.com"
                    },
                    "password": {
                        "description": "Password",
                        "type": "string",
                        "example": "Teszt1234"
                    }
                },
                "type": "object"
            },
            "AuthResource": {
                "title": "AuthResource",
                "description": "Token resource",
                "properties": {
                    "accessToken": {
                        "type": "string",
                        "example": "jwt_token"
                    },
                    "tokenType": {
                        "type": "string",
                        "example": "Bearer"
                    },
                    "expiration": {
                        "type": "integer",
                        "example": "36000"
                    }
                },
                "type": "object"
            },
            "UserResource": {
                "title": "UserResource",
                "description": "User Resource",
                "properties": {
                    "accessToken": {
                        "type": "string",
                        "example": "jwt_token"
                    }
                },
                "type": "object"
            }
        },
        "securitySchemes": {
            "bearerAuth": {
                "type": "http",
                "scheme": "bearer"
            }
        }
    }
}