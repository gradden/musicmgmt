    "components": {
        "schemas": {
            "ClubRequest": {
                "title": "ClubRequest",
                "description": "Full Club object request",
                "required": [
                    "name",
                    "location"
                ],
                "properties": {
                    "name": {
                        "description": "Club name",
                        "type": "string",
                        "example": "Liget Budapest"
                    },
                    "location": {
                        "description": "The location of the club",
                        "type": "string",
                        "example": "Budapest (Example street)"
                    },
                    "facebookUrl": {
                        "description": "Facebook page of the club",
                        "type": "string",
                        "example": "https://facebook.com/club"
                    },
                    "instagramTag": {
                        "description": "Instagram tag",
                        "type": "string",
                        "example": "@clubbudapest"
                    },
                    "instagramUrl": {
                        "description": "Instagram url",
                        "type": "string",
                        "example": "https://instagram.com/clubbudapest"
                    },
                    "description": {
                        "description": "A custom description",
                        "type": "string",
                        "example": "This place is very solid. Might be coming back here idk.."
                    }
                },
                "type": "object"
            },
            "LoginRequest": {
                "title": "LoginRequest",
                "description": "Login credentials",
                "required": [
                    "email",
                    "password"
                ],
                "properties": {
                    "email": {
                        "description": "Email",
                        "type": "string",
                        "example": "example@asd.com"
                    },
                    "password": {
                        "description": "Password",
                        "type": "string",
                        "example": "Teszt1234"
                    }
                },
                "type": "object"
            },
            "LogoutRequest": {
                "title": "LogoutRequest",
                "description": "Logout request",
                "required": [
                    "accessToken"
                ],
                "properties": {
                    "accessToken": {
                        "description": "Token",
                        "type": "string",
                        "example": "token"
                    }
                },
                "type": "object"
            },
            "RegisterRequest": {
                "title": "RegisterRequest",
                "description": "Registration credentials",
                "required": [
                    "name",
                    "email",
                    "password"
                ],
                "properties": {
                    "name": {
                        "description": "Full name",
                        "type": "string",
                        "example": "Foo Bar"
                    },
                    "email": {
                        "description": "Email",
                        "type": "string",
                        "example": "example@asd.com"
                    },
                    "password": {
                        "description": "Password",
                        "type": "string",
                        "example": "Teszt1234"
                    }
                },
                "type": "object"
            },
            "AuthResource": {
                "title": "AuthResource",
                "description": "Token resource",
                "properties": {
                    "accessToken": {
                        "type": "string",
                        "example": "jwt_token"
                    },
                    "tokenType": {
                        "type": "string",
                        "example": "Bearer"
                    },
                    "expiration": {
                        "type": "integer",
                        "example": "36000"
                    }
                },
                "type": "object"
            },
            "ClubIndexResource": {
                "title": "ClubIndexResource",
                "description": "Collection of Clubs",
                "properties": {
                    "data": {
                        "$ref": "#/components/schemas/ClubResource"
                    }
                },
                "type": "object"
            },
            "ClubResource": {
                "title": "ClubResource",
                "description": "Full resource of Club object",
                "properties": {
                    "name": {
                        "description": "Club name",
                        "type": "string",
                        "example": "Liget Budapest"
                    },
                    "location": {
                        "description": "The location of the club",
                        "type": "string",
                        "example": "Budapest (Example street)"
                    },
                    "facebookUrl": {
                        "description": "Facebook page of the club",
                        "type": "string",
                        "example": "https://facebook.com/club"
                    },
                    "instagramTag": {
                        "description": "Instagram tag",
                        "type": "string",
                        "example": "@clubbudapest"
                    },
                    "instagramUrl": {
                        "description": "Instagram url",
                        "type": "string",
                        "example": "https://instagram.com/clubbudapest"
                    },
                    "description": {
                        "description": "A custom description",
                        "type": "string",
                        "example": "This place is very solid. Might be coming back here idk.."
