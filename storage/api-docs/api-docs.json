    "components": {
        "schemas": {
            "ClubRequest": {
                "title": "ClubRequest",
                "description": "Full Club object request",
                "required": [
                    "name",
                    "location"
                ],
                "properties": {
                    "name": {
                        "description": "Club name",
                        "type": "string",
                        "example": "Liget Budapest"
                    },
                    "location": {
                        "description": "The location of the club",
                        "type": "string",
                        "example": "Budapest (Example street)"
                    },
                    "facebookUrl": {
                        "description": "Facebook page of the club",
                        "type": "string",
                        "example": "https://facebook.com/club"
                    },
                    "instagramTag": {
                        "description": "Instagram tag",
                        "type": "string",
                        "example": "@clubbudapest"
                    },
                    "instagramUrl": {
                        "description": "Instagram url",
                        "type": "string",
                        "example": "https://instagram.com/clubbudapest"
                    },
                    "description": {
                        "description": "A custom description",
                        "type": "string",
                        "example": "This place is very solid. Might be coming back here idk.."
                    }
                },
                "type": "object"
            },
            "ConcertRequest": {
                "title": "ConcertRequest",
                "description": "Full Concert object request",
                "required": [
                    "eventName",
                    "clubId",
                    "createdBy",
                    "eventStartDate",
                    "eventEndDate"
                ],
                "properties": {
                    "eventName": {
                        "description": "Event name",
                        "type": "string",
                        "example": "Nightlife Maximal"
                    },
                    "clubId": {
                        "description": "ID of the club",
                        "type": "integer",
                        "example": "1"
                    },
                    "createdBy": {
                        "description": "ID of the event creator (User ID)",
                        "type": "integer",
                        "example": "1"
                    },
                    "eventStartDate": {
                        "description": "Event's starting date",
                        "type": "string",
                        "example": "2023-03-01 12:00:00"
                    },
                    "eventEndDate": {
                        "description": "Event's ending date",
                        "type": "string",
                        "example": "2023-03-01 01:00:00"
                    },
                    "description": {
                        "description": "A custom description",
                        "type": "string",
                        "example": "Example description"
                    },
                    "income": {
                        "description": "Income of the gig",
                        "type": "integer",
                        "example": "90000"
                    },
                    "facebookUrl": {
                        "description": "Facebook event URL",
                        "type": "string",
                        "example": "https://facebook.com/events/1234"
                    },
                    "livesetUrl": {
                        "description": "Live Set URL",
                        "type": "string",
                        "example": "url"
                    },
                    "isExpired": {
                        "description": "Is expired",
                        "type": "boolean",
                        "example": "false"
                    }
                },
                "type": "object"
            },
            "LoginRequest": {
                "title": "LoginRequest",
                "description": "Login credentials",
                "required": [
                    "email",
                    "password"
                ],
                "properties": {
                    "email": {
                        "description": "Email",
                        "type": "string",
                        "example": "example@asd.com"
                    },
                    "password": {
                        "description": "Password",
                        "type": "string",
                        "example": "Teszt1234"
                    }
                },
                "type": "object"
            },
            "LogoutRequest": {
                "title": "LogoutRequest",
                "description": "Logout request",
                "required": [
                    "accessToken"
                ],
                "properties": {
                    "accessToken": {
                        "description": "Token",
                        "type": "string",
                        "example": "token"
                    }
                },
                "type": "object"
            },
            "RegisterRequest": {
                "title": "RegisterRequest",
                "description": "Registration credentials",
                "required": [
                    "name",
                    "email",
                    "password"
                ],
                "properties": {
                    "name": {
                        "description": "Full name",
                        "type": "string",
                        "example": "Foo Bar"
                    },
                    "email": {
                        "description": "Email",
                        "type": "string",
                        "example": "example@asd.com"
                    },
                    "password": {
                        "description": "Password",
                        "type": "string",
                        "example": "Teszt1234"
                    }
                },
                "type": "object"
            },
            "AuthResource": {
                "title": "AuthResource",
                "description": "Token resource",
                "properties": {
                    "accessToken": {
                        "type": "string",
                        "example": "jwt_token"
                    },
                    "tokenType": {
                        "type": "string",
                        "example": "Bearer"
                    },
                    "expiration": {
                        "type": "integer",
                        "example": "36000"
                    }
                },
                "type": "object"
            },
            "ClubIndexResource": {
                "title": "ClubIndexResource",
